{
  "name": "starter-webpack",
  "version": "1.0.0",
  "description": "My Enonic XP project",
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "build:client": "webpack --config webpack.client.config.js --color",
    "build:server": "webpack --config webpack.server.config.js --color",
    "build": "run-p -c build:*",
    "dist:client": "cross-env NODE_ENV=production webpack --config webpack.client.config.js --color",
    "dist:server": "cross-env NODE_ENV=production webpack --config webpack.server.config.js --color --hide-modules",
    "dist": "run-p -c dist:*"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/plugin-proposal-class-properties": "^7.16.0",
    "@babel/preset-env": "^7.15.8",
    "@babel/register": "^7.15.3",
    "autoprefixer": "^10.3.7",
    "babel-loader": "^8.2.3",
    "browserslist-config-enonic": "^1.0.6",
    "cross-env": "^7.0.3",
    "css-loader": "^6.4.0",
    "cssnano": "^5.0.8",
    "file-loader": "^6.2.0",
    "glob": "^7.2.0",
    "html-webpack-plugin": "^5.5.0",
    "less": "^4.1.2",
    "less-loader": "^10.2.0",
    "mini-css-extract-plugin": "^2.4.3",
    "node-sass": "^6.0.1",
    "postcss": "^8.3.11",
    "postcss-loader": "^6.2.0",
    "postcss-normalize": "^10.0.1",
    "ramda": "^0.27.1",
    "sass-loader": "^12.2.0",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.2.4",
    "ts-loader": "^9.2.6",
    "typescript": "^4.4.4",
    "webpack": "^5.60.0",
    "webpack-cli": "^4.9.1"
  },
  "browserslist": [
    "extends browserslist-config-enonic"
  ],
  "engines": {
    "node": ">= 14.15.0",
    "npm": ">= 6.14.8"
  }
}

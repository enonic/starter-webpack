{
  "name": "starter-webpack",
  "version": "1.0.0",
  "description": "My Enonic XP project",
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "build:client": "webpack --config webpack.client.config.js --color",
    "build:server": "webpack --config webpack.server.config.js --color",
    "build": "run-p -c build:*",
    "dist:client": "cross-env NODE_ENV=production webpack --config webpack.client.config.js --color",
    "dist:server": "cross-env NODE_ENV=production webpack --config webpack.server.config.js --color --hide-modules",
    "dist": "run-p -c dist:*"
  },
  "devDependencies": {
    "@babel/core": "^7.23.6",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.23.6",
    "@babel/register": "^7.22.15",
    "@enonic-types/global": "^7.13.5",
    "@enonic-types/lib-event": "^7.13.4",
    "autoprefixer": "^10.4.16",
    "babel-loader": "^9.1.3",
    "browserslist-config-enonic": "^1.0.8",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "cssnano": "^6.0.1",
    "file-loader": "^6.2.0",
    "glob": "^10.3.10",
    "html-webpack-plugin": "^5.5.3",
    "less": "^4.2.0",
    "less-loader": "^11.1.3",
    "mini-css-extract-plugin": "^2.7.6",
    "node-sass": "^9.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.32",
    "postcss-loader": "^7.3.3",
    "postcss-normalize": "^10.0.1",
    "ramda": "^0.29.1",
    "sass-loader": "^13.3.2",
    "style-loader": "^3.3.3",
    "terser-webpack-plugin": "^5.3.9",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "browserslist": [
    "extends browserslist-config-enonic"
  ],
  "engines": {
    "node": ">= 18.17.1",
    "npm": ">= 9.6.7"
  }
}
